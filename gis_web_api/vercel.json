{
    "builds" : [
        {
            "src" : "gis_web_api/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "gis_web_api/wsgi.python3"
        }
    ]
}
    
